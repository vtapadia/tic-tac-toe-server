{
  "name": "tic-tac-toe-server",
  "version": "1.0.0",
  "description": "Tic Tac Toe Service in Nodejs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtapadia/tic-tac-toe-server.git"
  },
  "keywords": [
    "nodejs"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "author": "Varesh Tapadia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vtapadia/tic-tac-toe-server/issues"
  },
  "homepage": "https://github.com/vtapadia/tic-tac-toe-server#readme",
  "dependencies": {
    "@hapi/hapi": "^19.1.1",
    "hapi-pino": "^8.0.1"
  },
  "devDependencies": {
    "@types/hapi": "^18.0.3",
    "@types/node": "^14.0.5",
    "copyfiles": "^2.3.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules"
    ],
    "ext": "ts",
    "exec": "npm start",
    "watch": [
      "src"
    ],
    "verbose": true
  }
}
